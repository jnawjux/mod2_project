    "import pandas as pd\n",
    "conn = sqlite3.connect(\"music.sqlite\")\n",
    "cur = conn.cursor()\n",
    "temporaryX = cur.execute(\"SELECT * FROM genres\").fetchall()\n",
    "df = pd.DataFrame(temporaryX)"
